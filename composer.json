{
    "name": "tgachet/kata-php-template",
    "description": "kata-php-template",
    "type": "project",
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "behat/behat": "^3.5",
        "phpstan/phpstan": "^0.11.15",
        "symfony/var-dumper": "^4.3",
        "fzaninotto/faker": "^1.8",
        "behat/mink": "^1.7",
        "behat/mink-goutte-driver": "^1.2",
        "behat/mink-extension": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "App\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\" : "tests/"
        }
    }
}
